{
  "name": "auto-deploy-rest-api",
  "version": "1.0.0",
  "description": "This application includes auto deploy rest services.",
  "main": "App.js",
  "scripts": {
    "start": "nodemon -L --inspect=0.0.0.0:9229 App.js --exec 'npm run lint-autofix && node'",
    "start:prod": "node App.js",
    "lint": "./node_modules/.bin/eslint . --ext .js",
    "lint-autofix": "./node_modules/.bin/eslint . --ext .js --fix",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check"
  },
  "author": "ogeday26",
  "_moduleAliases": {
    "@src": "src",
    "@schemas": "src/schemas",
    "@utils": "src/utils",
    "@config": "config",
    "@controllers": "src/controllers",
    "@middlewares": "src/middlewares"
  },
  "license": "ISC",
  "dependencies": {
    "@sentry/node": "^5.5.0",
    "axios": "^0.18.0",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-handlebars": "^3.1.0",
    "express-jwt": "^5.3.1",
    "helmet": "^3.16.0",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.0",
    "minio": "^7.0.5",
    "module-alias": "^2.2.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.25",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "nodemailer": "^5.1.1",
    "pdfkit": "^0.9.1",
    "pg": "^7.8.2",
    "puppeteer": "^1.17.0",
    "redis": "^2.8.0",
    "secure-random-password": "^0.1.0",
    "socket.io": "^2.2.0",
    "socketio-jwt": "^4.5.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.15.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "pretty-error": "^2.1.1"
  }
}
